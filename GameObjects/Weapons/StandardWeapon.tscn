script/source = "extends Node2D

func _ready():
	$ani.connect(\"animation_finished\", self, \"ani_done\")
	$ani.play(\"stay\")

func shoot():
	$ani.play(\"shoot\")

func _physics_process(delta):
	if Input.is_action_just_pressed(\"ui_accept\"):
		shoot()

func ani_done(a):
	if (a != \"stay\"):
		$ani.play(\"stay\")"

